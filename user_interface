def _run_user_interface()->None:
  _togo_list=[]
  for i in _location_count():
    _togo_list.extend(input())
  for i in _togo_list:
    print(i)
    
def _location_count()->int:
	while True:
		count = input()
		try:
			int_count=int(count.strip())
		except:
			print('Invalid input! Try again.')
		else:
			if int_count<=1:
				print('Invalid input! Try again.')
			else:
				return int_count

if __name__ == '__main__':
  _run_user_interface()
